# Function to get and save ACS data for a given year and location
get_and_save_acs_data <- function(year, location) {
state_code <- substr(location, nchar(location)-1, nchar(location))
place_name <- substr(location, 1, nchar(location)-4)
acs_data <- get_acs(
geography = "place",
variables = c("B01001_001", "B01001_002", "B01001_026", "B01001_003", "B01001_004", "B01001_005", "B01001_006", "B01001_007", "B01001_008", "B01001_009", "B01001_010", "B01001_011", "B01001_012", "B01001_013", "B01001_014", "B01001_015", "B01001_016", "B01001_017", "B01001_018", "B01001_019", "B01001_020", "B01001_021", "B01001_022", "B01001_023", "B01001_024", "B01001_025", "B15003_001", "B19013_001"),  # <-- Ensure this list contains your desired variables
year = year,
state = state_code,
place = place_name
)
# Check if data is retrieved
if (nrow(acs_data) == 0) {
message(paste("No data found for", location, "in", year))
return(NULL)
}
# Rename the variables for better interpretation
acs_data <- acs_data %>%
mutate(variable = case_when(
variable == "B01001_001" ~ "Total Population",
variable == "B01001_002" ~ "Male Population",
variable == "B01001_026" ~ "Female Population",
variable %in% c("B01001_003", "B01001_004", "B01001_005", "B01001_006") ~ "Children (0-17)",
variable %in% c("B01001_007", "B01001_008", "B01001_009", "B01001_010") ~ "Young Adults (18-24)",
variable %in% c("B01001_011", "B01001_012", "B01001_013", "B01001_014", "B01001_015", "B01001_016", "B01001_017", "B01001_018", "B01001_019", "B01001_020", "B01001_021") ~ "Adults (25-64)",
variable %in% c("B01001_022", "B01001_023", "B01001_024", "B01001_025") ~ "Seniors (65+)",
variable == "B15003_001" ~ "Educational Attainment",
variable == "B19013_001" ~ "Median Household Income",
TRUE ~ variable
))
# Aggregate the age groups
acs_data <- acs_data %>%
group_by(NAME, variable) %>%
summarize(estimate = sum(estimate, na.rm = TRUE)) %>%
ungroup()
# Reshape the data
acs_data_reshaped <- acs_data %>%
select(NAME, variable, estimate) %>%
pivot_wider(names_from = variable, values_from = estimate) %>%
rename(
Location = NAME
)
# Save the reshaped data to a CSV file
write_csv(acs_data_reshaped, paste0("acs_data_", year, "_", gsub(", ", "_", location), ".csv"))
# View the head of the data
print(head(acs_data_reshaped))
}
# Loop through years and locations to get and save ACS data
years <- 2020:2024
for (year in years) {
for (location in locations) {
get_and_save_acs_data(year, location)
}
}
